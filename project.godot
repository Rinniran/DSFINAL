[application]

config/name="DreamShock"
config/description="A blast through space, time, and dreams!"
run/main_scene="res://Subrooms/GamepadNotice.tscn"
run/low_processor_mode=true
boot_splash/image="res://Yaiba mini boss Illustration + logo.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Icon.png"
config/windows_native_icon="res://icon (2).ico"

[autoload]

Globals="*res://src/globals.gd"
Settings="*res://src/Settings.gd"
Sys="*res://src/Sys.gd"
Discord="*res://addons/discord_game_sdk/discord.gd"

[debug]

shapes/collision/contact_color=Color( 0, 0.00392157, 0.188235, 0.8 )

[display]

window/size/width=384
window/size/height=224
window/size/test_width=768
window/size/test_height=640
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

main_run_args="--verbose"

[editor_plugins]

enabled=PoolStringArray( "res://addons/discord_game_sdk/plugin.cfg", "res://addons/smoothing/plugin.cfg" )

[global]

path=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

