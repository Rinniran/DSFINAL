resource_name = "Yaibabossend"
script/source = "extends AnimationPlayer


# Declare member variables here. Examples:
# var a = 2
# var b = \"text\"

var didit = 0
var didit2 = 0
# Called when the node enters the scene tree for the first time.
func _ready():
	pass # Replace with function body.


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta):
	if Switches.YaibaDefeated == 1:
		if didit != 1:
			play(\"beat yaiba\")
			didit = 1
	if Switches.stg1complete == true:
		if didit2 != 1:
			clear_caches()
			clear_queue()
			play(\"rideoff\")
			didit2 = 1
	
	print_debug(Switches.stg1complete)
#	pass
"

